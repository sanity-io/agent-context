# Sanity Agent Context MCP URL
# Format: https://api.sanity.io/vX/agent-context/PROJECT_ID/DATASET/SLUG
SANITY_CONTEXT_MCP_URL=

# Sanity API read token (used to authenticate with Context MCP)
SANITY_API_READ_TOKEN=

# Sanity project info (used for image CDN URLs in widgets)
SANITY_PROJECT_ID=
SANITY_DATASET=production

# Server port (default: 3001)
PORT=3001
